{
  "name": "toffice",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . t-office --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"T-Office\"",
    "package-linux": "electron-packager . t-office --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Darko Milutinovic (DARCO SOLUTIONS) <dmilutinovic@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "electron": "^1.8.2"
  },
  "devDependencies": {
    "@types/angular": "^1.6.42"
  }
}
